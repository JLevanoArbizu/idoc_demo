	<queryString language="SQL">
		<![CDATA[SELECT
APEPATPER,APEMATPER,NOMPER, DNIPER, DISTUBI, DIRPER, REPLACE(REPLACE(GENPER,'M','Masculino'),'F','Femenino') AS GENPER ,
CONVERT(VARCHAR(20),FECINITRAB, 106) AS FECINITRAB , CONVERT(VARCHAR(20),FECFINTRAB, 106) AS FECFINTRAB
FROM General.TRABAJADOR
INNER JOIN General.AREA ON TRABAJADOR.IDARE = AREA.IDARE
INNER JOIN General.PERSONA ON TRABAJADOR.IDPER = PERSONA.IDPER
INNER JOIN General.UBIGEO ON PERSONA.CODUBI = UBIGEO.CODUBI]]>
	</queryString>
